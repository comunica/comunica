    "portCache": 3000,
    "workers": 4,
    "maxMemory": 2084,
    "dataset": "generated/dataset.hdt",
    "hookSparqlEndpointLdfEngine": {"@id": "urn:jbr:benchmark-bsbm-tpf:combination_1:hookSparqlEndpoint_hookSparqlEndpointLdfEngine",  "@type": "HookSparqlEndpointComunica",  "dockerfileClient": "input/dockerfiles/Dockerfile-client",  "resourceConstraints": {"@type": "StaticDockerResourceConstraints","cpu_percentage": 100  },  "configClient": "input/config-client.json",  "contextClient": "input/context-client.json",  "additionalBinds": [],  "clientPort": 3001,  "clientLogLevel": "info",  "maxMemory": 8192}
  }
