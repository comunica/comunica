{
  "name": "@comunica/runner-cli",
  "version": "1.4.4",
  "description": "A comunica runner that sends an 'init' event from a commandline process.",
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/comunica/comunica/tree/master/packages/runner-cli",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "runner",
    "cli"
  ],
  "license": "MIT",
  "bin": {
    "comunica-run": "./bin/run.js"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://github.com/comunica/comunica#readme",
  "files": [
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@comunica/runner": "^1.4.4"
  },
  "devDependencies": {
    "@comunica/bus-init": "^1.4.0",
    "@comunica/core": "^1.4.0"
  },
  "scripts": {
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  }
}
