{
  "name": "@comunica/bus-rdf-metadata-extract",
  "version": "1.22.0",
  "description": "A comunica bus for rdf-metadata-extract events.",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/bus-rdf-metadata-extract"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "bus",
    "rdf-metadata-extract"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "dependencies": {
    "@comunica/types": "^1.22.0",
    "@rdfjs/types": "*",
    "graphql-ld": "^1.4.0",
    "rdf-store-stream": "^1.3.0",
    "sparqlalgebrajs": "^4.0.0",
    "stream-to-string": "^1.2.0"
  },
  "peerDependencies": {
    "@comunica/actor-init-sparql": "^1.16.2",
    "@comunica/core": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "build:components-bus": "componentsjs-generator -s lib --typeScopedContexts -i ../../.componentsignore"
  }
}
