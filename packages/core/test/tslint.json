{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "array-type": [true, "array"],
    "indent": [true, "spaces"],
    "ter-indent": [true, 2],
    "no-empty-interface": false,
    "quotemark": false,
    "no-trailing-whitespace": [true, "ignore-jsdoc"],
    "valid-jsdoc": [false, {
      "prefer": {
        "return": "return"
      },
      "requireReturn": false
    }],
    "no-angle-bracket-type-assertion": false,

    "no-var-requires": false,
    "no-unused-expression": false
  },
  "rulesDirectory": []
}