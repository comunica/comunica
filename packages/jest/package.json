{
  "name": "@comunica/jest",
  "version": "3.1.0",
  "description": "Jest utilities for Comunica",
  "license": "MIT",
  "homepage": "https://comunica.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/jest"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "keywords": [
    "jest",
    "rdf",
    "comunica",
    "testing",
    "testing",
    "matchers",
    "extend"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "typings": "lib/index",
  "publishConfig": {
    "access": "public"
  },
  "module": "lib/index.mjs",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.js.map"
  ],
  "scripts": {
    "build": "yarn run build:ts",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "rollup": "rollup --config rollup.config.mjs"
  },
  "dependencies": {
    "@comunica/bindings-factory": "^3.1.0",
    "@comunica/types": "^3.1.0",
    "@rdfjs/types": "*",
    "arrayify-stream": "^2.0.1"
  }
}
