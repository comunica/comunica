{
  "name": "@comunica/bus-http",
  "version": "1.22.1",
  "description": "A comunica bus for 'http' events.",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/bus-http"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "bus",
    "init"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "peerDependencies": {
    "@comunica/core": "^1.0.0"
  },
  "dependencies": {
    "@comunica/context-entries": "^1.22.0",
    "@types/readable-stream": "^2.3.11",
    "is-stream": "^2.0.0",
    "readable-web-to-node-stream": "^3.0.2",
    "web-streams-node": "^0.4.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components-bus",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "build:components-bus": "componentsjs-generator -s lib --typeScopedContexts -i ../../.componentsignore"
  }
}
