{
  "name": "@comunica/bus-query-operation",
  "version": "1.1.0",
  "description": "A comunica bus for query-operation events.",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/bus-query-operation",
  "lsd:components": "components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/contexts/comunica-bus-query-operation.jsonld": "components/context.jsonld"
  },
  "main": "index.js",
  "typings": "index",
  "repository": "https://github.com/comunica/comunica/tree/master/packages/bus-query-operation",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "bus",
    "query-operation"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://github.com/comunica/comunica#readme",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "asynciterator": "^2.0.0",
    "immutable": "^3.8.2"
  },
  "peerDependencies": {
    "@comunica/core": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/core": "^1.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls"
  }
}
