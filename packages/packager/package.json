{
  "name": "@comunica/packager",
  "version": "3.1.0",
  "description": "A tool for packaging Comunica modules in a single NPM package",
  "license": "MIT",
  "homepage": "https://comunica.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/packager"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "keywords": [
    "comunica",
    "packager"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "module": "bin/package.mjs",
  "bin": {
    "comunica-package": "./bin/package.js"
  },
  "files": [
    "bin/package.d.ts",
    "bin/package.js",
    "bin/package.js.map"
  ],
  "scripts": {
    "rollup": "tsc && cp -r ./dist ./bin && rm -rf ./dist"
  },
  "dependencies": {
    "@types/minimist": "^1.2.2",
    "componentsjs": "^6.0.1",
    "minimist": "^1.2.7",
    "process": "^0.11.10"
  }
}
