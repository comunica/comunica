{
  "name": "sparqlee",
  "version": "1.6.0",
  "description": "A simple SPARQL expression evaluator library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "tslint --project .",
    "lint-test": "tslint ./test/**/*.ts",
    "test": "jest",
    "test-ci": "jest --ci --maxWorkers=4 --coverage",
    "test-debug": "yarn test-debug-lin",
    "test-debug-lin": "node --inspect-brk=5858 ./node_modules/.bin/jest",
    "test-debug-win": "node --inspect-brk=5858 ./node_modules/jest/bin/jest.js",
    "coveralls-ci": "cat ./coverage/lcov.info | coveralls",
    "prepare": "yarn run build",
    "preversion": "yarn test",
    "version": "manual-git-changelog onversion",
    "postversion": "git push && git push --tags",
    "sparqlee": "./dist/bin/Sparqlee.js"
  },
  "bin": {
    "sparqlee": "dist/bin/Sparqlee.js"
  },
  "files": [
    "/dist/bin",
    "/dist/lib",
    "/dist/index.d.ts",
    "/dist/index.js.map"
  ],
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comunica/sparqlee.git"
  },
  "keywords": [
    "sparql",
    "expression",
    "rdf",
    "evaluator"
  ],
  "author": "Wout Schellaert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/sparqlee/issues"
  },
  "homepage": "https://github.com/comunica/sparqlee#readme",
  "devDependencies": {
    "@types/benchmark": "^1.0.30",
    "@types/jest": "^26.0.0",
    "asynciterator": "^3.0.0",
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.2",
    "jest": "^26.0.0",
    "manual-git-changelog": "^1.0.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/rdf-js": "^4.0.0",
    "@types/spark-md5": "^3.0.2",
    "@types/uuid": "^8.0.0",
    "decimal.js": "^10.2.0",
    "hash.js": "^1.1.7",
    "immutable": "^3.8.2",
    "rdf-data-factory": "^1.0.3",
    "rdf-string": "^1.5.0",
    "relative-to-absolute-iri": "^1.0.6",
    "spark-md5": "^3.0.1",
    "sparqlalgebrajs": "^2.4.0",
    "uuid": "^8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/lib/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
