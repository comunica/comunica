{
  "name": "@comunica/actor-rdf-serialize-n3",
  "version": "1.22.0",
  "description": "A n3 rdf-serialize actor",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-rdf-serialize-n3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "rdf-serialize",
    "n3"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/actor-rdf-serialize-n3/issues"
  },
  "homepage": "https://github.com/comunica/actor-rdf-serialize-n3#readme",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "dependencies": {
    "@rdfjs/types": "*",
    "@types/n3": "^1.4.4",
    "n3": "^1.6.3",
    "rdf-string": "^1.5.0"
  },
  "peerDependencies": {
    "@comunica/bus-rdf-serialize": "^1.0.0",
    "@comunica/core": "^1.0.0"
  },
  "devDependencies": {
