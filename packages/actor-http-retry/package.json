  },
  "dependencies": {
    "@comunica/bus-http": "^4.0.0",
    "@comunica/bus-http-invalidate": "^4.0.0",
    "@comunica/context-entries": "^4.0.0",
    "@comunica/core": "^4.0.0",
    "@comunica/mediatortype-time": "^4.0.0"
  }
}
