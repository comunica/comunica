  },
  "main": "index.js",
  "typings": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comunica/actor-init-rdf-dereference.git"
  },
  "keywords": [
    "comunica",
    "runner"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/actor-init-rdf-dereference/issues"
  },
