{
  "name": "@comunica/actor-init-rdf-dereference",
  "version": "0.0.1",
  "description": "An RDF Parse init actor",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/actor-init-rdf-dereference",
