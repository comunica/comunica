    "@comunica/actor-rdf-parse-jsonld": "^0.0.1",
    "@comunica/bus-init": "^0.0.1",
    "@comunica/bus-http": "^0.0.1",
    "@comunica/bus-rdf-dereference": "^0.0.1",
    "@comunica/bus-rdf-parse": "^0.0.1",
    "@comunica/core": "^0.0.1",
