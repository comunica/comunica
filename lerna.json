{
  "lerna": "2.4.0",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md"
      ],
      "message": "Bump to release version %s",
      "allowBranch": "master"
    }
  },
  "packages": [
    "packages/*"
  ],
  "useWorkspaces": true,
  "version": "1.3.0",
  "loglevel": "success",
  "registry": "https://registry.npmjs.org/",
  "npmClient": "yarn"
}
